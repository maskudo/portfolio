{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "jsx-quotes": 0,
    "react/react-in-jsx-scope": 0,
    "react/self-closing-comp": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "comma-dangle": 0
  }
}
